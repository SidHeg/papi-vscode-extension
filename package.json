{
  "name": "papi",
  "displayName": "Property Manager",
  "description": "Extension for Akamai Property Manager",
  "version": "0.9.0-beta.0",
  "publisher": "Akamai",
  "engines": {
    "vscode": "^1.47.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SidHeg/papi-vscode-extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:papi.edit",
    "onCommand:papi.validate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "papi.edit",
        "title": "Akamai Property Manager: Edit Rules"
      },
      {
        "command": "papi.validate",
        "title": "Akamai Property Manager: Validate Rules"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "pretty": "prettier --write \"./**/*.{ts,js,jsx,json}\""
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.1.5",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/request": "^2.48.5",
    "@types/underscore": "^1.10.18",
    "@types/uuid": "^8.3.0",
    "crypto": "1.0.1",
    "json-schema-to-typescript": "^9.1.1",
    "jsonschema": "^1.2.6",
    "moment": "^2.27.0",
    "request": "^2.88.2",
    "underscore": "^1.10.2",
    "url": "0.11.0",
    "uuid": "^8.2.0",
    "vscode-languageclient": "^6.1.3"
  }
}
